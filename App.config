  <ProductAlphaFolders>
    <add key="0" value="patch" />
  </ProductAlphaFolders>
  <ProductBetaFolders>
    <add key="0" value="cases" />
  </ProductBetaFolders>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>